{
    "manifest_version": 2,
   
    "name": "Youtube Genius Search",
    "version": "1.0",
    "description": "Search Lyrics of YouTube Videos on Genius",

    "permissions": [
      "activeTab",
      "https://api.genius.com/*"
    ],

    "browser_action": {
      "default_icon": "./icon.png",
      "default_title": "Search Lyrics on Genius"
    },

    "background": {
      "scripts": ["background.js"]
    }
  }

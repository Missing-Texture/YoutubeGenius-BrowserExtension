{
    "manifest_version": 2,
   
    "name": "Youtube Genius",
    "version": "1.0",
    "description": "Search Lyrics of YouTube Videos on Genius",

    "permissions": ["tabs","https://api.genius.com/*", "webRequest"],
   
    "content_scripts": [{
      "css": ["styles.css"],
      "js": ["content.js"],
      "matches": ["https://www.youtube.com/*"]
    }]
  }